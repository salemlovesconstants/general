local Players = Game:GetService("Players")
local Player = Players.LocalPlayer
coroutine.wrap(function()
    while wait(0.5) do pcall(function() 
 firetouchinterest(game:GetService("Players").LocalPlayer.Character.Head, game:GetService("Workspace").Banknote.Handle, 0)
end)
end
end)()
coroutine.wrap(function()
      while wait(0.1) do pcall(function() 
     local Humanoid = game:GetService("Players").LocalPlayer.Character:FindFirstChild("Humanoid") 
    local Tool = game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Banknote")
    Humanoid:EquipTool(Tool)
  end)
  end
end)()
coroutine.wrap(function()
 while wait(0.5) do pcall(function() 
 firetouchinterest(game:GetService("Players").LocalPlayer.Character.Head, game:GetService("Workspace").BiggerBanknote.Handle, 0)
end)
end
end)()
